<AnchorPane prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.streetcycle.CRUD.JenisServiceInputController">
   <children>
      <ImageView fx:id="imageView" fitHeight="1080.0" fitWidth="1920.0" layoutX="4.0" layoutY="4.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Gambar/JnsService.jpg" />
         </image>
      </ImageView>
      <Label layoutX="597.0" layoutY="319.0" text="ID Jenis Service" textFill="WHITE">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="597.0" layoutY="408.0" text="Id Alat" textFill="WHITE">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="597.0" layoutY="495.0" text="Nama Jenis Service" textFill="#fffefe">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="596.0" layoutY="574.0" text="Harga Service" textFill="WHITE">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="txtId" layoutX="964.0" layoutY="294.0" prefHeight="49.0" prefWidth="366.0">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="txtNamaJenis" layoutX="964.0" layoutY="470.0" prefHeight="49.0" prefWidth="366.0">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="txtHarga" layoutX="964.0" layoutY="563.0" prefHeight="49.0" prefWidth="366.0">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Button fx:id="btnSave" layoutX="749.0" layoutY="796.0" mnemonicParsing="false" onAction="#onBtnSaveClick" prefHeight="39.0" prefWidth="360.0" text="Save">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnClear" layoutX="749.0" layoutY="858.0" mnemonicParsing="false" onAction="#onBtnClearClick" prefHeight="39.0" prefWidth="360.0" text="Clear">
         <font>
            <Font size="18.0" />
