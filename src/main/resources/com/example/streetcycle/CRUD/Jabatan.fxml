      </Label>
      <Button fx:id="btnSimpan" layoutX="629.0" layoutY="468.0" mnemonicParsing="false" onAction="#onbtnSimpanClick" prefHeight="25.0" prefWidth="212.0" text="Save" />
      <Button fx:id="btnBatal" layoutX="629.0" layoutY="509.0" mnemonicParsing="false" onAction="#onbtnBatalClick" prefHeight="25.0" prefWidth="212.0" text="Cancel" />
